 "html": "<style>\r\n    @media print {\r\n        @page {\r\n            size: 4in 6in; /* \u0623\u0648 100mm 150mm */\r\n            margin: 0;\r\n        }\r\n        body {\r\n            margin: 0;\r\n            padding: 0;\r\n        }\r\n    }\r\n    * {\r\n        box-sizing: border-box;\r\n        margin: 0;\r\n        padding: 0;\r\n    }\r\n    .shipping-label {\r\n        width: 100%;\r\n        max-width: 4in;\r\n        font-family: 'Arial', 'Tahoma', sans-serif;\r\n        direction: rtl;\r\n        font-size: 8px;\r\n        color: #000;\r\n        padding: 4px;\r\n    }\r\n\r\n    /* === HEADER === */\r\n    .header-row {\r\n        display: table;\r\n        width: 100%;\r\n        margin-bottom: 2px;\r\n    }\r\n    .header-right {\r\n        display: table-cell;\r\n        width: 55%;\r\n        vertical-align: top;\r\n        text-align: right;\r\n    }\r\n    .header-left {\r\n        display: table-cell;\r\n        width: 45%;\r\n        vertical-align: middle;\r\n        text-align: left;\r\n    }\r\n    .date-box {\r\n        border: 2px solid #A9A9A9;\r\n        padding: 3px 6px;\r\n        display: inline-block;\r\n        margin-bottom: 2px;\r\n    }\r\n    .date-box .date-label {\r\n        font-weight: bold;\r\n        font-size: 10px;\r\n        color: #A9A9A9;\r\n    }\r\n    .date-box .date-value {\r\n        font-size: 9px;\r\n        font-weight: bold;\r\n        direction: ltr;\r\n        display: inline-block;\r\n    }\r\n    .order-box {\r\n        border: 2px solid #A9A9A9;\r\n        padding: 3px 6px;\r\n        display: inline-block;\r\n    }\r\n    .order-box .order-label {\r\n        font-weight: bold;\r\n        font-size: 8px;\r\n        color: #A9A9A9;\r\n    }\r\n    .order-box .order-value {\r\n        font-size: 8px;\r\n        font-weight: bold;\r\n    }\r\n    .brand-area {\r\n        text-align: center;\r\n    }\r\n    .brand-area .brand-name {\r\n        font-size: 8px;\r\n        font-weight: bold;\r\n        color: #A9A9A9;\r\n    }\r\n    .brand-area .brand-sub {\r\n        font-size: 7px;\r\n        color: #666;\r\n    }\r\n\r\n    /* === TITLE === */\r\n    .label-title {\r\n        text-align: center;\r\n        font-size: 10px;\r\n        font-weight: bold;\r\n        margin: 6px 0;\r\n        color: #000;\r\n    }\r\n\r\n    /* === TABLES === */\r\n    table.info-table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n        margin-bottom: 2px;\r\n    }\r\n    table.info-table td {\r\n        border: 1.5px solid #000;\r\n        padding: 2px 3px;\r\n        vertical-align: middle;\r\n    }\r\n    table.info-table .label-cell {\r\n        width: 38%;\r\n        font-weight: bold;\r\n        text-align: center;\r\n        background-color: #f8f8f8;\r\n        font-size: 7px;\r\n    }\r\n    table.info-table .value-cell {\r\n        width: 62%;\r\n        font-size: 6px;\r\n        min-height: 5px;\r\n    }\r\n\r\n    /* === SHIPMENT SECTION === */\r\n    .section-header {\r\n        font-weight: bold;\r\n        font-size: 9px;\r\n        padding: 2px 6px;\r\n        margin-bottom: 0;\r\n    }\r\n    table.items-table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n        margin-bottom: 4px;\r\n    }\r\n    table.items-table th {\r\n        border: 1.5px solid #000;\r\n        padding: 3px 4px;\r\n        text-align: center;\r\n        font-weight: bold;\r\n        font-size: 8px;\r\n        background-color: #f8f8f8;\r\n    }\r\n    table.items-table td {\r\n        border: 1.5px solid #000;\r\n        padding: 3px 4px;\r\n        text-align: center;\r\n        font-size: 8px;\r\n        min-height: 5px;\r\n    }\r\n    table.items-table td:first-child {\r\n        text-align: right;\r\n    }\r\n\r\n    /* === BOTTOM SECTION === */\r\n    .bottom-section {\r\n        display: table;\r\n        width: 100%;\r\n        margin-bottom: 4px;\r\n    }\r\n    .bottom-right {\r\n        display: table-cell;\r\n        width: 55%;\r\n        vertical-align: top;\r\n        padding-left: 4px;\r\n    }\r\n    .bottom-left {\r\n        display: table-cell;\r\n        width: 45%;\r\n        vertical-align: top;\r\n    }\r\n    .notes-box {\r\n        border: 1.5px solid #000;\r\n        padding: 4px 6px;\r\n        font-size: 8px;\r\n        height: 100%;\r\n        min-height: 55px;\r\n    }\r\n    .notes-box .notes-title {\r\n        font-weight: bold;\r\n        font-size: 8px;\r\n        margin-bottom: 3px;\r\n    }\r\n    .fragile-line {\r\n        font-size: 9px;\r\n        margin-top: 4px;\r\n    }\r\n    table.totals-table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n    }\r\n    table.totals-table td {\r\n        border: 1.5px solid #000;\r\n        padding: 3px 4px;\r\n        font-size: 8px;\r\n    }\r\n    table.totals-table .total-label {\r\n        font-weight: bold;\r\n        text-align: center;\r\n        background-color: #f8f8f8;\r\n        width: 50%;\r\n    }\r\n    table.totals-table .total-value {\r\n        text-align: center;\r\n        width: 50%;\r\n    }\r\n\r\n    /* === REMARKS === */\r\n    .remarks-section {\r\n        border: 1.5px solid #000;\r\n        padding: 4px 6px;\r\n        margin-bottom: 4px;\r\n        min-height: 16px;\r\n        font-size: 9px;\r\n        background: #f0f0f0;\r\n    }\r\n    .remarks-section strong {\r\n        color: #A9A9A9;\r\n    }\r\n\r\n    /* === FOOTER === */\r\n    .footer-section {\r\n        text-align: center;\r\n        font-size: 9px;\r\n        margin-top: 4px;\r\n        padding-top: 3px;\r\n        border-top: 1px dashed #000;\r\n    }\r\n    .footer-section .thank-you {\r\n        font-weight: bold;\r\n        font-size: 10px;\r\n        margin-top: 3px;\r\n    }\r\n    .phone-icon {\r\n        display: inline-block;\r\n        width: 10px;\r\n        height: 10px;\r\n        margin: 0 2px;\r\n    }\r\n</style>\r\n\r\n<div class=\"shipping-label\">\r\n\r\n    <!-- ===== HEADER ===== -->\r\n    <div class=\"header-row\">\r\n        <div class=\"header-right\">\r\n            <div class=\"date-box\">\r\n                <span class=\"date-label\">\u0627\u0644\u062a\u0627\u0631\u064a\u062e</span>\r\n                <span class=\"date-value\">{{ doc.posting_date }}</span>\r\n            </div>\r\n            <br>\r\n            <div class=\"order-box\">\r\n                <span class=\"order-label\">\u0631\u0642\u0645 \u0637\u0644\u0628 \u0627\u0644\u0628\u064a\u0639</span>\r\n                <span class=\"order-value\">{% if doc.items %}{{ doc.items[0].against_sales_order or doc.name }}{% else %}{{ doc.name }}{% endif %}</span>\r\n            </div>\r\n            <div class=\"order-box\">\r\n                <span class=\"order-label\">\u0631\u0642\u0645 \u062a\u062a\u0628\u0639 \u0627\u0644\u0634\u062d\u0646\u0629 </span>\r\n                <span class=\"order-value\">{{ doc.tracking_shipment_number }}</span>\r\n            </div>\r\n        </div>\r\n        <div class=\"header-left\">\r\n            <div class=\"brand-area\">\r\n                <div class=\"brand-name\">{{doc.company}}</div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- ===== TITLE ===== -->\r\n    <div class=\"label-title\">\u0628\u0648\u0644\u064a\u0635\u0629 \u0627\u0644\u0634\u062d\u0646</div>\r\n\r\n    <!-- ===== CUSTOMER INFO ===== -->\r\n    <table class=\"info-table\">\r\n        <tr>\r\n            <td class=\"label-cell\">\u0627\u0633\u0645 \u0627\u0644\u0639\u0645\u064a\u0644</td>\r\n            <td class=\"value-cell\">{{ doc.shopify_shipping_customer_name or doc.customer_name or '' }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td class=\"label-cell\">\u0627\u0644\u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u062a\u0641\u0635\u064a\u0644\u064a</td>\r\n            <td class=\"value-cell\">{{ doc.shopify_shipping_address or doc.shipping_address or doc.address_display  or '' }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td class=\"label-cell\">\u0631\u0642\u0645 \u0627\u0644\u0647\u0627\u062a\u0641</td>\r\n            <td class=\"value-cell\">{{ doc.shopify_shipping_phone or doc.contact_mobile or '' }}</td>\r\n        </tr>\r\n    </table>\r\n\r\n    <!-- ===== SHIPMENT DETAILS ===== -->\r\n    <div class=\"section-header\">\u062a\u0641\u0627\u0635\u064a\u0644 \u0627\u0644\u0634\u062d\u0646\u0629:</div>\r\n    <table class=\"items-table\">\r\n        <thead>\r\n            <tr>\r\n                <th style=\"width:40%;\">\u0627\u0633\u0645 \u0627\u0644\u0645\u0646\u062a\u062c</th>\r\n                <th style=\"width:12%;\">\u0639\u062f\u062f</th>\r\n                <th style=\"width:24%;\">\u0633\u0639\u0631 \u0627\u0644\u0645\u0646\u062a\u062c</th>\r\n                <th style=\"width:24%;\">\u0623\u062c\u0645\u0627\u0644\u064a</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            {%- for item in doc.items -%}\r\n            <tr>\r\n                <td>{{ item.item_name }}</td>\r\n                <td>{{ item.qty | int }}</td>\r\n                <td>{{ item.rate }}</td>\r\n                <td>{{ item.amount }}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n\r\n    <!-- ===== BOTTOM: NOTES + TOTALS ===== -->\r\n    <div class=\"bottom-section\">\r\n        <div class=\"bottom-right\">\r\n            <div class=\"notes-box\">\r\n                <div class=\"notes-title\">\u0645\u0644\u0627\u062d\u0638\u0627\u062a \u0627\u0644\u0634\u062d\u0646\u0629:</div>\r\n                <div class=\"fragile-line\">\u0627\u0644\u0645\u0646\u062a\u062c \u0642\u0627\u0628\u0644 \u0644\u0644\u0643\u0633\u0631: &nbsp;&nbsp; <strong>\u0646\u0639\u0645</strong> &nbsp;-&nbsp; <strong>\u0644\u0627</strong></div>\r\n            </div>\r\n        </div>\r\n        <div class=\"bottom-left\">\r\n            <table class=\"totals-table\">\r\n                <!--<tr>-->\r\n                <!--    <td class=\"total-label\">\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a</td>-->\r\n                <!--    <td class=\"total-value\">{{ doc.total }}</td>-->\r\n                <!--</tr>-->\r\n                <!--<tr>-->\r\n                <!--    <td class=\"total-label\">\u0627\u0644\u0634\u062d\u0646</td>-->\r\n                <!--    <td class=\"total-value\">{% if doc.taxes %}{{ doc.total_taxes_and_charges }}{% else %}0.00{% endif %}</td>-->\r\n                <!--</tr>-->\r\n                <tr>\r\n                    <td class=\"total-label\">\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a \u0627\u0644\u0643\u0644\u0649</td>\r\n                    <td class=\"total-value\" style=\"font-weight:bold; font-size:11px;\">{{ doc.grand_total }}</td>\r\n                </tr>\r\n            </table>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- ===== REMARKS ===== -->\r\n    <div class=\"remarks-section\">\r\n        <strong>\u0645\u0644\u0627\u062d\u0638\u0627\u062a:</strong>\r\n        {{ doc.instructions or '' }}\r\n    </div>\r\n\r\n    <!-- ===== CONTACT FOOTER ===== -->\r\n    <div class=\"remarks-section\" style=\"background: #A9A9A9; color: #fff; text-align: center; font-size: 8px; padding: 3px;\">\r\n        \u0628\u0631\u062c\u0627\u0621 \u0627\u0644\u062a\u0648\u0627\u0635\u0644 \u0639\u0644\u0649 \u0627\u0644\u0635\u0641\u062d\u0629 \u0627\u0630\u0627 \u0647\u0646\u0627\u0643 \u0627\u064a \u0645\u0634\u0643\u0644\u0629 \u0641\u0649 \u0627\u0644\u0627\u0648\u0631\u062f\u0631\r\n   </div>\r\n\r\n    <!-- ===== THANK YOU ===== -->\r\n    <div class=\"footer-section\">\r\n        <div class=\"thank-you\">\u0634\u0643\u0631\u0627 \u0644\u062a\u0633\u0648\u0642\u0643\u0645 \u0645\u0639\u0646\u0627</div>\r\n    </div>\r\n\r\n</div>",
